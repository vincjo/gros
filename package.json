{
    "name": "gros",
    "version": "1.0.0-runes.5",
    "keywords": [
        "Jeu de composants Svelte r√©utilisables"
    ],
    "description": "GROS ui toolkit",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vincjo/gros.git"
