    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vincjo/gros/issues"
    },
    "homepage": "https://vincjo.fr/gros",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "package": "svelte-kit sync && svelte-package",
