        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "deploy": "pm2 deploy ecosystem.config.cjs production"
    },
    "devDependencies": {
        "@sveltejs/adapter-node": "^5.0.0",
        "@sveltejs/package": "^2.3.1",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@types/node": "^18.15.11",
        "@vincjo/datatables": "^2.0.0-runes.2",
        "mdsvex": "^0.11.2",
        "prism-svelte": "^0.5.0",
        "svelte": "^5.0.0-next.148",
        "svelte-check": "^3.4.3",
        "svelte-preprocess": "^5.0.3",
        "tslib": "^2.3.1",
        "typescript": "^5.0.0",
        "vite": "^5.0.0"
    },
    "type": "module",
    "files": [
        "dist"
    ],
    "dependencies": {
        "@neodrag/svelte": "^2.0.1",
        "@popperjs/core": "^2.11.6",
        "@sveltejs/kit": "^2.0.0",
        "sortablejs": "^1.15.0",
        "svelte": "^3.56.0 || ^4.0.0 || ^5.0.0-next.120",
        "svelte-loading-spinners": "^0.3.4"
    },
    "exports": {
        "./action": {
            "types": "./dist/action/index.d.ts",
            "default": "./dist/action/index.js"
        },
        "./datatable": {
            "types": "./dist/datatable/index.d.ts",
            "default": "./dist/datatable/index.js"
        },
        "./date": {
